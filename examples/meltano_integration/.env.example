# FLEXT Target Oracle - Meltano Integration Environment Variables
# Copy this file to .env and configure with your Oracle credentials

# Oracle Database Configuration (Required)
TARGET_ORACLE_ORACLE_HOST=localhost
TARGET_ORACLE_ORACLE_PASSWORD=secure_password
TARGET_ORACLE_ORACLE_PORT=1521
TARGET_ORACLE_ORACLE_SERVICE=XE
TARGET_ORACLE_ORACLE_USER=meltano_user

TARGET_ORACLE_BATCH_SIZE=2000
TARGET_ORACLE_CONNECTION_TIMEOUT=60
# Oracle Target Configuration (Optional)
TARGET_ORACLE_DEFAULT_TARGET_SCHEMA=MELTANO_DATA
TARGET_ORACLE_LOAD_METHOD=BULK_INSERT
TARGET_ORACLE_USE_BULK_OPERATIONS=true

# CSV Tap Configuration (Example)
TAP_CSV_FILES="[{\"entity\": \"customers\", \"path\": \"data/customers.csv\", \"keys\": [\"id\"]}]"

# Meltano Configuration
MELTANO_ENVIRONMENT=dev
MELTANO_PROJECT_ROOT=/path/to/your/project
